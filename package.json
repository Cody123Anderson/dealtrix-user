{
  "name": "serenade-user",
  "version": "1.0.0",
  "description": "User portion of Serenade Dates",
  "main": "server.js",
  "scripts": {
    "dev": "export NODE_ENV=development && webpack --watch --progress",
    "lint": "eslint . --ext .js --fix && npm run lint:sass",
    "lint:sass": "sass-lint --config .sass-lint.yml --verbose --no-exit",
    "deploy": "./deploy.sh",
    "prod": "export NODE_ENV=production && webpack --progress",
    "start": "export NODE_ENV=development && webpack-dev-server --port 3002 --progress",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cody123Anderson/serenade-user.git"
  },
  "keywords": [
    "react",
    "landing",
    "page",
    "sendgrid"
  ],
  "author": "cody123anderson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cody123Anderson/serenade-user/issues"
  },
  "homepage": "https://github.com/Cody123Anderson/serenade-user#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "node-sass": "^4.5.1",
    "postcss-loader": "^1.3.3",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-modal": "^1.7.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-truncate": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
